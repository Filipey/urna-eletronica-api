{
  "name": "urna-eletronica-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "lint": "npx eslint --fix src/*.ts",
    "build": "tsc && node dist/server.js"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/index.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Filipey/urna-eletronica-api.git"
  },
  "keywords": [],
  "author": "Filipe Augusto Santos de Moura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Filipey/urna-eletronica-api/issues"
  },
  "homepage": "https://github.com/Filipey/urna-eletronica-api#readme",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
